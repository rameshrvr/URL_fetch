{
    "manifest_version": 2,
    "name": "URL Fetch",
    "version": "1.0",
    "description": "Gets URL information from web browser",
    // "icons": {
    //   "128": "icon_16.png",
    //   "128": "icon_32.png",
    //   "128": "icon_48.png",
    //   "128": "icon_128.png"
    // },
    // "background": {
    //   "persistent": false,
    //   "scripts": ["background_script.js"]
    // },
    "permissions": ["https://*.google.com/", "activeTab", "http://127.0.0.1:5000/"],
    "content_security_policy":"script-src 'self' https://apis.google.com; object-src 'self'",
    "browser_action": {
      "default_popup": "welcome.html",
      "default_icon": "asserts/images/url_icon.png"
    }
  }